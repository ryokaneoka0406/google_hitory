{
    "manifest_version": 2,
    "name": "History Viewer",
    "version": "0.2.1",
    "description": "Show your browser history",
    "author": "ryopenguin",
    "icons": {
        "16": "icons/myicon.png",
        "48": "icons/myicon.png",
        "128": "icons/myicon.png"
    },
    "browser_action": {
        "default_icon": "icons/myicon.png",
        "default_title": "History Viewer",
        "default_popup": "popup.html"
    },
    "permissions": [
        "history",
        "tabs"
    ]
}